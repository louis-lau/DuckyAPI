{
  "name": "ducky-api",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "webpack --color --config webpack.config.prod.js",
    "build:dev": "webpack --color --config webpack.config.dev.js",
    "start": "wait-on dist/main.js && node dist/main",
    "start:dev": "wait-on dist/main.js && nodemon --watch config/development.env --exec \"node --unhandled-rejections=strict dist/main\"",
    "start:debug": "wait-on dist/main.js && nodemon --watch config/development.env --exec \"node --inspect-brk dist/main\"",
    "buildstart": "npm-run-all --silent --print-label clean build start",
    "buildstart:dev": "npm-run-all --silent --print-label clean --parallel build:dev start:dev",
    "buildstart:debug": "npm-run-all --silent --print-label clean --parallel build:dev start:debug",
    "duckypanel:install": "git clone https://github.com/louis-lau/DuckyPanel.git duckypanel && npm run duckypanel:update",
    "duckypanel:update": "cd duckypanel && git fetch && git checkout d1b3456b6a6c0898634277f424c055c355f80516 && npm install && npm run build",
    "docs:generate": "ts-node scripts/update-docs.ts",
    "lint": "npm-run-all --silent --print-label lint:tsc lint:eslint",
    "lint:fix": "npm-run-all --silent --print-label lint:tsc lint:eslintfix",
    "lint:tsc": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "lint:eslint": "eslint --color --max-warnings 0 ./ --ext ts",
    "lint:eslintfix": "eslint --fix --color --max-warnings 0 ./ --ext ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/bull": "0.1.1",
    "@nestjs/common": "^7.0.13",
    "@nestjs/config": "^0.4.2",
    "@nestjs/core": "^7.0.13",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.13",
    "@nestjs/serve-static": "^2.1.1",
    "@nestjs/swagger": "^4.5.6",
    "@nestjs/typeorm": "^7.0.0",
    "bcrypt": "^4.0.1",
    "bull": "^3.14.0",
    "bull-arena": "^2.8.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "commander": "^5.1.0",
    "express-basic-auth": "^1.2.0",
    "generate-password": "^1.5.1",
    "helmet": "^3.22.0",
    "mongodb": "^3.5.7",
    "nanoid": "^3.1.7",
    "nestjs-console": "^2.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "snyk": "^1.332.1",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.1.5",
    "@nestjs/testing": "^7.0.13",
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.13.0",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^17.1.0",
    "@types/helmet": "0.0.47",
    "@types/jest": "^25.2.2",
    "@types/mongoose": "^5.7.20",
    "@types/node": "^14.0.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.9",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "axios": "^0.19.2",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "openapi-snippet": "^0.8.1",
    "prettier": "^2.0.5",
    "prettier-eslint": "^9.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.0.0",
    "ts-loader": "^7.0.4",
    "ts-node": "8.10.1",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "3.9.2",
    "wait-on": "^5.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "snyk": true
}
